{
  "seed": 9,
  "dataset": {
    "name": "snli",
    "use_lemmatized": true
  },
  "embeddings": {
    "main": {
      "dim": 300,
      "file": "gcc840.h5",
      "trainable": false,
      "vocab": "default"
    }
  },
  "streams": {
    "train": {
      "batch_size": 32,
      "fraction": 0.001,
      "shuffle": true
    },
    "dev": {
      "batch_size": 133,
      "fraction": 1.0,
      "shuffle": false
    },
    "test": {
      "batch_size": 307,
      "fraction": 1.0,
      "shuffle": false
    },
    "breaking": {
      "batch_size": 3,
      "fraction": 1.0,
      "shuffle": false
    }
  },
  "batch_transformers": [
    {
      "name": "wordnet",
      "file": "pair_features.pkl"
    },
    {
      "name": "indexer",
      "vocab": "default"
    },
    {
      "name": "padder"
    }
  ],
  "vocabs": {
    "default": {
      "file_or_data": "vocab_all.txt",
      "specials_check": true
    }
  },
  "trainer": {
    "clip_norm": 10.0,
    "learning_rate": 0.0004,
    "learning_rate_scheduler": {
        "type": "reduce_on_plateau",
        "factor": 0.5,
        "mode": "max",
        "patience": 1
    },
    "loss": "categorical_crossentropy",
    "metrics": ["accuracy"],
    "num_epochs": 50,
    "optimizer": "adam",
    "patience": 7,
    "save_best": true,
    "save_every_epoch": false
  },

  "model": {
    "D": 0,
    "batch_sizes": {
        "snli": {
            "train": 32,
            "dev": 133,
            "test": 307
        },
        "mnli": {
            "train": 32,
            "dev": 65,
            "test": 8
        },
        "breaking": {
            "test": 3
        }
    },
    "shuffle": {
        "snli": {
            "train": true
        },
        "mnli": {
            "train": true
        }
    },
    "centering": false,
    "clip_gradient_norm": 5.0,
    "cudnn": true,
    "dataset": "mnli",
    "dropout": 0.5,
    "early_stopping": true,
    "elmo_dir": "elmo",
    "elmo_use_weighted_embeddings": true,
    "elmo_use_layer_normalization": true,
    "elmo_after_lstm": true,
    "embedding_dim": 300,
    "embedding_name": "gcc840",
    "embedding_second_name": "gcc840",
    "fullkim": false,
    "a_lambda": 20,
    "i_lambda": 1.0,
    "intersection_of_embedding_dicts": false,
    "knowledge_after_lstm": "none",
    "learning_rate": 0.0004,
    "l2_elmo_regularization": 0.001,
    "l2_weight_regularization": 0.0,
    "lr_schedule": "[[1000, 0.0004]]",
    "lr_schedule_type": "reduce_on_plateau",
    "model": "esim",
    "n_epochs": 60,
    "n_labels": 3,
    "n_layers": 3,
    "norm_weight": true,
    "normalize": false,
    "optimizer": "adam",
    "pair_features_pkl_path": "pair_features.pkl",
    "pair_features_txt_path": "kim_data/pair_features.txt",
    "residual_embedding": {
        "active": false,
        "type": "none",
        "normalize": false
    },
    "save_best_model": true,
    "save_every_epoch": false,
    "save_model": false,
    "seed": 9,
    "sentence_max_length": 90,
    "steps_per_epoch_scale": 1.0,
    "train_embeddings": false,
    "train_on_fraction": 1.0,
    "use_elmo": false,
    "useatrick": 1,
    "useitrick": 1,
    "usectrick": 0,
    "whitening": false
  }
}